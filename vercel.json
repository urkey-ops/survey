{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/submit-survey",
      "destination": "/api/submit-survey.js"
    },
    {
      "source": "/api/sync-analytics",
      "destination": "/api/sync-analytics.js"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/index.html",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
